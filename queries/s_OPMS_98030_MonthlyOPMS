SELECT TOP 24 s_OPMS_98013_ReportsOut.Month, s_OPMS_98015_SamplesIn.Received, s_OPMS_98013_ReportsOut.Samples AS Reports, s_OPMS_98014_Activity.Activity, s_OPMS_98016_Fails.CountOfResultID AS Fails, s_OPMS_98020_ProbandTAT.CountOfArrayTestID AS Probands, s_OPMS_98020_ProbandTAT.Count42days AS Probands42, s_OPMS_98020_ProbandTAT.[42days] AS ProbandsPercent, s_OPMS_98020_ProbandTAT.TATMeanAll AS ProbandsMean, s_OPMS_98021_UrgentTAT.CountOfArrayTestID AS Urgent, s_OPMS_98021_UrgentTAT.Count14days AS Urgent14, s_OPMS_98021_UrgentTAT.[14days] AS UrgentPercent, s_OPMS_98021_UrgentTAT.TATMeanAll AS UrgentMean, s_OPMS_98022_InheritanceTAT.CountOfArrayTestID AS Inheritance, s_OPMS_98022_InheritanceTAT.Count42days AS Inheritance42, s_OPMS_98022_InheritanceTAT.[42days] AS InheritancePercent, s_OPMS_98022_InheritanceTAT.TATMeanAll AS InheritanceMean
FROM (((((s_OPMS_98014_Activity INNER JOIN s_OPMS_98013_ReportsOut ON s_OPMS_98014_Activity.Month = s_OPMS_98013_ReportsOut.Month) LEFT JOIN s_OPMS_98016_Fails ON s_OPMS_98014_Activity.Month = s_OPMS_98016_Fails.Month) INNER JOIN s_OPMS_98020_ProbandTAT ON s_OPMS_98014_Activity.Month = s_OPMS_98020_ProbandTAT.Month) INNER JOIN s_OPMS_98021_UrgentTAT ON s_OPMS_98014_Activity.Month = s_OPMS_98021_UrgentTAT.Month) INNER JOIN s_OPMS_98022_InheritanceTAT ON s_OPMS_98014_Activity.Month = s_OPMS_98022_InheritanceTAT.Month) INNER JOIN s_OPMS_98015_SamplesIn ON s_OPMS_98014_Activity.Month = s_OPMS_98015_SamplesIn.Month
GROUP BY s_OPMS_98013_ReportsOut.Month, s_OPMS_98015_SamplesIn.Received, s_OPMS_98013_ReportsOut.Samples, s_OPMS_98014_Activity.Activity, s_OPMS_98016_Fails.CountOfResultID, s_OPMS_98020_ProbandTAT.CountOfArrayTestID, s_OPMS_98020_ProbandTAT.Count42days, s_OPMS_98020_ProbandTAT.[42days], s_OPMS_98020_ProbandTAT.TATMeanAll, s_OPMS_98021_UrgentTAT.CountOfArrayTestID, s_OPMS_98021_UrgentTAT.Count14days, s_OPMS_98021_UrgentTAT.[14days], s_OPMS_98021_UrgentTAT.TATMeanAll, s_OPMS_98022_InheritanceTAT.CountOfArrayTestID, s_OPMS_98022_InheritanceTAT.Count42days, s_OPMS_98022_InheritanceTAT.[42days], s_OPMS_98022_InheritanceTAT.TATMeanAll
ORDER BY s_OPMS_98013_ReportsOut.Month DESC;
