SELECT TOP 18 s_OPMS_98013_Samples.Month, s_OPMS_98013_Samples.Samples, s_OPMS_98014_Activity.Activity, s_OPMS_98016_Fails.CountOfResultID AS Fails, s_OPMS_98020_ProbandTAT.CountOfArrayTestID AS Probands, s_OPMS_98020_ProbandTAT.Count28days AS Probands28, s_OPMS_98020_ProbandTAT.[28days] AS ProbandsPercent, s_OPMS_98021_UrgentTAT.CountOfArrayTestID AS Urgent, s_OPMS_98021_UrgentTAT.Count14days AS Urgent14, s_OPMS_98021_UrgentTAT.[14days] AS UrgentPercent, s_OPMS_98022_InheritanceTAT.CountOfArrayTestID AS Inheritance, s_OPMS_98022_InheritanceTAT.Count28days AS Inheritance28, s_OPMS_98022_InheritanceTAT.[28days] AS InheritancePercent
FROM ((((s_OPMS_98013_Samples INNER JOIN s_OPMS_98014_Activity ON s_OPMS_98013_Samples.Month = s_OPMS_98014_Activity.Month) LEFT JOIN s_OPMS_98016_Fails ON s_OPMS_98013_Samples.Month = s_OPMS_98016_Fails.Month) INNER JOIN s_OPMS_98020_ProbandTAT ON s_OPMS_98013_Samples.Month = s_OPMS_98020_ProbandTAT.Month) INNER JOIN s_OPMS_98021_UrgentTAT ON s_OPMS_98013_Samples.Month = s_OPMS_98021_UrgentTAT.Month) INNER JOIN s_OPMS_98022_InheritanceTAT ON s_OPMS_98013_Samples.Month = s_OPMS_98022_InheritanceTAT.Month
GROUP BY s_OPMS_98013_Samples.Month, s_OPMS_98013_Samples.Samples, s_OPMS_98014_Activity.Activity, s_OPMS_98016_Fails.CountOfResultID, s_OPMS_98020_ProbandTAT.CountOfArrayTestID, s_OPMS_98020_ProbandTAT.Count28days, s_OPMS_98020_ProbandTAT.[28days], s_OPMS_98021_UrgentTAT.CountOfArrayTestID, s_OPMS_98021_UrgentTAT.Count14days, s_OPMS_98021_UrgentTAT.[14days], s_OPMS_98022_InheritanceTAT.CountOfArrayTestID, s_OPMS_98022_InheritanceTAT.Count28days, s_OPMS_98022_InheritanceTAT.[28days]
ORDER BY s_OPMS_98013_Samples.Month DESC;
