SELECT ConsumableStock.ConsumableStockID, ConsumableStock.ConsumableID, ConsumableStock.InternalOrderNo, ConsumableStock.ExternalOrderNo, ConsumableStock.DateOrdered, ConsumableStock.QuantityOrdered, ConsumableStock.LotNo, ConsumableStock.KitLotNo, ConsumableStock.DateReceived, ConsumableStock.ExpiryDate, ConsumableStock.Finished, ConsumableStock.SectionID, ConsumableStock.AuthorisedForUseBy, ConsumableStock.StockUsed, Consumable.Active, Status.Status, Item.Item, Checker.Name, Checker_1.Name, Checker_2.Name, ConsumableStock.StockUsed
FROM (((((ConsumableStock LEFT JOIN Consumable ON ConsumableStock.ConsumableID = Consumable.ConsumableID) LEFT JOIN Status ON Consumable.Active = Status.StatusID) LEFT JOIN Item ON ConsumableStock.SectionID = Item.ItemID) LEFT JOIN Checker ON ConsumableStock.OrderedFor = Checker.Check1ID) LEFT JOIN Checker AS Checker_1 ON ConsumableStock.OrderedBy = Checker_1.Check1ID) LEFT JOIN Checker AS Checker_2 ON ConsumableStock.AuthorisedForUseBy = Checker_2.Check1ID
WHERE (((ConsumableStock.ExpiryDate)<DateAdd("m",3,Date())) AND ((Consumable.Active)=1202218774) AND ((ConsumableStock.StockUsed)=False))
ORDER BY ConsumableStock.ExpiryDate;
