SELECT Mid(TempPreNat.Chr,4,2) AS C, Mid(TempPreNat.Chr,4,2) & TempPreNat.[Band] AS CytoBand, TempPreNat.Start, TempPreNat.Stop, TempPreNat.Probe, TempPreNat.RatioPos, TempPreNat.RatioNeg, GenomeSyndrome.PreNat, TempPreNat.Stop-TempPreNat.Start+1 AS [Size]
FROM TempPreNat, GenomeSyndrome INNER JOIN Chromosome ON GenomeSyndrome.ChrID19 = Chromosome.ChrID
WHERE (((TempPreNat.Start)<=[GenomeSyndrome].[Start19]) AND ((TempPreNat.Stop)>=[GenomeSyndrome].[Start19]) AND ((GenomeSyndrome.PreNat)=1896 Or (GenomeSyndrome.PreNat)=1897 Or (GenomeSyndrome.PreNat)=1898) AND ((Mid([TempPreNat].[Chr],4,2))=[Chromosome].[Chr])) OR (((TempPreNat.Start)<=[GenomeSyndrome].[Stop19]) AND ((TempPreNat.Stop)>=[GenomeSyndrome].[Stop19]) AND ((GenomeSyndrome.PreNat)=1896 Or (GenomeSyndrome.PreNat)=1897 Or (GenomeSyndrome.PreNat)=1898) AND ((Mid([TempPreNat].[Chr],4,2))=[Chromosome].[Chr])) OR (((TempPreNat.Start)>=[GenomeSyndrome].[Start19]) AND ((TempPreNat.Stop)<=[GenomeSyndrome].[Stop19]) AND ((GenomeSyndrome.PreNat)=1896 Or (GenomeSyndrome.PreNat)=1897 Or (GenomeSyndrome.PreNat)=1898) AND ((Mid([TempPreNat].[Chr],4,2))=[Chromosome].[Chr]));
