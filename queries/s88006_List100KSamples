SELECT NGSTest.Selection, Patients.PatientID AS PRU, NGSTest.NGSTestID, dbo_Patient_table.LastName, dbo_Patient_table.FirstName, dbo_Patient_table.DoB, gw_GenderTable.GenderShort, NGSTest.DateRequested, NGSPanel.Panel AS PanelA, NGSPanel_1.Panel AS PanelB, Status_1.StatusID AS PatientStatus, NGSTest.ResultComment, Patients.InternalPatientID, NGSTest.Check1Date, NGSTest.Check2Date, NGSTest.Check3Date, Patients.s_StatusOverall, NGSPanel.PanelCode AS [Primary], NGSPanel_1.PanelCode AS Secondary, NGSTest.DNA, NGSTest.Priority, NGSTest.Result AS TestResult, NGSPanel_2.PanelCode AS Tertiary, NGSTest.Check4Date, NGSTest.GELProbandID, NGSTest.IRID, NGSTest.GWWorksheet, NGSTest.MDTdate, NGSTest.Check1ID, NGSTest.Check2ID, NGSTest.Check3ID, NGSTest.Check4ID, NGSTest.BookBy, NGSTest.StatusID, Checker.Name AS ReferringClinician, NGSTest.ResultCode
FROM (((((((NGSTest INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) LEFT JOIN NGSPanel ON NGSTest.NGSPanelID = NGSPanel.NGSPanelID) LEFT JOIN NGSPanel AS NGSPanel_1 ON NGSTest.NGSPanelID_B = NGSPanel_1.NGSPanelID) INNER JOIN dbo_Patient_table ON Patients.PatientID = dbo_Patient_table.PatientTrustID) INNER JOIN Status AS Status_1 ON Patients.s_StatusOverall = Status_1.StatusID) LEFT JOIN gw_GenderTable ON dbo_Patient_table.GenderID = gw_GenderTable.GenderID) LEFT JOIN NGSPanel AS NGSPanel_2 ON NGSTest.NGSPanelID_C = NGSPanel_2.NGSPanelID) LEFT JOIN Checker ON NGSTest.BookBy = Checker.Check1ID
WHERE (((NGSTest.ReferralID)=1199901218))
ORDER BY NGSTest.Priority, NGSTest.DateRequested;
