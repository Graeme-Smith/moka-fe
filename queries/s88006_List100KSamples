SELECT Patients.PatientID AS PRU, NGSTest.NGSTestID, dbo_Patient_table.LastName, dbo_Patient_table.FirstName, dbo_Patient_table.DoB, gw_GenderTable.GenderShort, NGSTest.DateRequested, Status_1.StatusID AS PatientStatus, NGSTest.ResultComment, Patients.InternalPatientID, NGSTest.Check1Date, NGSTest.Check2Date, NGSTest.Check3Date, Patients.s_StatusOverall, NGSTest.DNA, NGSTest.Priority, NGSTest.Result AS TestResult, NGSTest.Check4Date, NGSTest.GELProbandID, NGSTest.IRID, NGSTest.GWWorksheet, NGSTest.MDTdate, NGSTest.Check1ID, NGSTest.Check2ID, NGSTest.Check3ID, NGSTest.Check4ID, NGSTest.BookBy, NGSTest.StatusID, Checker.Name AS ReferringClinician, NGSTest.ResultCode
FROM ((((NGSTest INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN dbo_Patient_table ON Patients.PatientID = dbo_Patient_table.PatientTrustID) INNER JOIN Status AS Status_1 ON Patients.s_StatusOverall = Status_1.StatusID) LEFT JOIN gw_GenderTable ON dbo_Patient_table.GenderID = gw_GenderTable.GenderID) LEFT JOIN Checker ON NGSTest.BookBy = Checker.Check1ID
WHERE (((NGSTest.ReferralID)=1199901218))
ORDER BY NGSTest.Priority, NGSTest.DateRequested;
