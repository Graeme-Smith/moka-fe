SELECT Format([ArrayTest].[ArrayResultDate],"yyyy") & "-" & Format(Format([ArrayTest].[ArrayResultDate],"mm"),"00") AS [Month], Checker.Initials, ArrayTest.Check2ID
FROM (ResultCode INNER JOIN ArrayTest ON ResultCode.ResultCodeID = ArrayTest.ArrayResultCodeID) INNER JOIN Checker ON ArrayTest.Check2ID = Checker.Check1ID
WHERE (((ArrayTest.Service)=True) AND ((ArrayTest.ReferralID)=2 Or (ArrayTest.ReferralID)=1199901195 Or (ArrayTest.ReferralID)=1185356886 Or (ArrayTest.ReferralID)=1199901176 Or (ArrayTest.ReferralID)=1199901197 Or (ArrayTest.ReferralID)=1199901175 Or (ArrayTest.ReferralID)=1199901204 Or (ArrayTest.ReferralID)=1199901171 Or (ArrayTest.ReferralID)=1199901199 Or (ArrayTest.ReferralID)=1199901185 Or (ArrayTest.ReferralID)=1199901188 Or (ArrayTest.ReferralID)=1199901202 Or (ArrayTest.ReferralID)=1199901179 Or (ArrayTest.ReferralID)=1199901181 Or (ArrayTest.ReferralID)=1199901187 Or (ArrayTest.ReferralID)=1199901194) AND ((ArrayTest.ArrayResultCodeID)=1 Or (ArrayTest.ArrayResultCodeID)=1189679610 Or (ArrayTest.ArrayResultCodeID)=1189679611 Or (ArrayTest.ArrayResultCodeID)=1189679612 Or (ArrayTest.ArrayResultCodeID)=1189679614 Or (ArrayTest.ArrayResultCodeID)=1189679663) AND ((ArrayTest.RequestedDate)>Now()-740));
