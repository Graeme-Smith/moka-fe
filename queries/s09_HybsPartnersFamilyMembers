SELECT Referral.Referral, Patients.PatientID, ArrayTest_to_filter.ArrayTestID, Patients.CandidateAbnormality AS FollowupRegion, [Patients].[bookinLastName] & " " & [Patients].[bookinFirstname] AS Name, s_HybIDs_minimum_info.HybID, s_HybIDs_minimum_info_to_filter.InternalPatientID
FROM (ArrayTest AS ArrayTest_1 INNER JOIN Referral ON ArrayTest_1.ReferralID = Referral.ReferralID) INNER JOIN (Patients INNER JOIN s_HybIDs_minimum_info ON Patients.InternalPatientID = s_HybIDs_minimum_info.InternalPatientID) ON ArrayTest_1.InternalPatientID = s_HybIDs_minimum_info.InternalPatientID, (ArrayTest AS ArrayTest_to_filter INNER JOIN s_HybIDs_minimum_info AS s_HybIDs_minimum_info_to_filter ON ArrayTest_to_filter.DNALabellingID = s_HybIDs_minimum_info_to_filter.DNALabellingID) INNER JOIN Patients AS Patients_1 ON s_HybIDs_minimum_info_to_filter.InternalPatientID = Patients_1.InternalPatientID
WHERE (((Left([Patients].[PatientID],6))=Left([Patients_1].[patientID],6)));
