SELECT DISTINCT Checker.Name AS [Referring Clinician], NGSTest.GELProbandID AS [GEL participant ID], NGSTest.IRID, "https://cipapi.genomicsengland.nhs.uk/interpretationportal/#/participant/" & [NGSTest].[IRID] & "/tiering" AS Review_variants_URL, Status.Status, Patients.BookinSex AS Sex, NGSTest.NGSTestID, NGSTest.InternalPatientID, dbo_Patient_Table.LastName, dbo_Patient_Table.FirstName, dbo_Patient_Table.DoB, dbo_Patient_Table.PatientTrustID, dbo_DNATestRequestLinked.Disorder, dbo_SpecimenCostLinked.Cost, dbo_SpecimenCostLinked.DNAUnitsTotal, dbo_SpecimenCostLinked.TestType
FROM (((NGSTest INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) LEFT JOIN Checker ON NGSTest.BookBy = Checker.Check1ID) INNER JOIN Status ON NGSTest.StatusID = Status.StatusID) LEFT JOIN ((dbo_DNATestRequestLinked RIGHT JOIN (dbo_specimenlinked LEFT JOIN dbo_SpecimenCostLinked ON dbo_specimenlinked.SpecimenID = dbo_SpecimenCostLinked.SpecimenID) ON dbo_DNATestRequestLinked.TestID = dbo_SpecimenCostLinked.TestID) RIGHT JOIN dbo_Patient_Table ON dbo_specimenlinked.PatientID = dbo_Patient_Table.PatientID) ON Patients.PatientID = dbo_Patient_Table.PatientTrustID
WHERE (((dbo_SpecimenCostLinked.Cost)=150 Or (dbo_SpecimenCostLinked.Cost) Is Null) AND ((dbo_SpecimenCostLinked.DNAUnitsTotal)=1 Or (dbo_SpecimenCostLinked.DNAUnitsTotal) Is Null) AND ((dbo_SpecimenCostLinked.TestType)="GEL NEG" Or (dbo_SpecimenCostLinked.TestType) Is Null) AND ((NGSTest.ReferralID)=1199901218) AND ((NGSTest.StatusID)<>4 And (NGSTest.StatusID)<>1202218814) AND ((NGSTest.ResultCode)=1189679668) AND ((dbo_DNATestRequestLinked.DisorderID)=60 Or (dbo_DNATestRequestLinked.DisorderID) Is Null));
