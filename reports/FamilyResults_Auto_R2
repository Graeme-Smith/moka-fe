SELECT DISTINCT ArrayTest.ArrayTestID, ArrayTest.InternalPatientID, ArrayTest.SpecimenNo, Patients.PatientID, ResultCode.ResultCode, Patients.OverallResultComments, [dbo_PatientLinked].[LastName] & " " & [dbo_PatientLinked].[FirstName] AS Name, dbo_PatientLinked.FamilyTrustID AS Family, dbo_PatientLinked.DoB, Left([dbo_PatientLinked].[Gender],1) AS Sex, dbo_FamilyNotesTraceLinked.FamilyID AS HasNotes
FROM (((ArrayTest INNER JOIN Patients ON ArrayTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN dbo_PatientLinked ON Patients.PatientID = dbo_PatientLinked.PatientTrustID) LEFT JOIN dbo_FamilyNotesTraceLinked ON dbo_PatientLinked.FamilyID = dbo_FamilyNotesTraceLinked.FamilyID) INNER JOIN ResultCode ON Patients.OverallResultCodeID = ResultCode.ResultCodeID;
