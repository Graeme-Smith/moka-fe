SELECT DISTINCT ArrayTest.ArrayTestID, Referral.Referral, ArrayTest.Priority, ArrayTest.SpecimenNo, Patients.InternalPatientID, Patients.PatientID, [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] AS Name, [gw_GenderTable].[GenderShort] & [patients].[sexed] AS Sex, dbo_Patient_Table.DoB, Left([patients].[patientid],6) AS FamilyNo, dbo_Specimen_Table.DateReceived, gw_SpecimenType.SpecimenTypeCode AS SpecimenType, [s_GW_SpecimenClinicianHospitalNoHospital].[Clinician] & " " & [s_GW_SpecimenClinicianHospitalNoHospital].[Hospital] AS ClinicianHospital, s_GW_FamilyIDFamilyNoPinkNotes.PinkNotes AS Pink
FROM (((((((Patients INNER JOIN ArrayTest ON Patients.InternalPatientID = ArrayTest.InternalPatientID) INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID) INNER JOIN dbo_Patient_Table ON Patients.PatientID = dbo_Patient_Table.PatientTrustID) LEFT JOIN gw_GenderTable ON dbo_Patient_Table.GenderID = gw_GenderTable.GenderID) INNER JOIN dbo_Specimen_Table ON ArrayTest.SpecimenNo = dbo_Specimen_Table.SpecimenTrustID) INNER JOIN s_GW_SpecimenClinicianHospitalNoHospital ON dbo_Specimen_Table.SpecimenID = s_GW_SpecimenClinicianHospitalNoHospital.SpecimenID) INNER JOIN gw_SpecimenType ON dbo_Specimen_Table.SpecimenTypeID = gw_SpecimenType.SpecimenTypeID) INNER JOIN s_GW_FamilyIDFamilyNoPinkNotes ON dbo_Patient_Table.FamilyID = s_GW_FamilyIDFamilyNoPinkNotes.FamilyID;
