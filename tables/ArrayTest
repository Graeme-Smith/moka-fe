	[Check2Date] [datetime] NULL,
	[Check3ID] [int] NULL,
	[Check3Date] [datetime] NULL,
	[Check4ID] [int] NULL,
	[Check4Date] [datetime] NULL,
	[Check5ID] [int] NULL,
	[Check5Date] [datetime] NULL,
	[ArrayTestAnalysisTypeID] [int] NULL,
	[DyeSwapID] [int] NULL,
	[DNALabellingID] [int] NULL,
	[Processing] [nvarchar](255) NULL,
	[Fail] [bit] NOT NULL CONSTRAINT [DF__ArrayTest__Fail__1920BF5C]  DEFAULT ((0)),
	[ReportID] [int] NULL,
	[ReportDate] [datetime] NULL,
