SELECT distinct dbo_patientlinked.PatientTrustID AS [PRU No], dbo_patientlinked.LastName AS Surname, dbo_patientlinked.FirstName AS [First name], dbo_specimenlinked.SpecimenTrustID AS [Spec no], dbo_dnanumberlinked.DNANo AS [DNA #], " " AS [Vol sent], dbo_dnasendawaylinked.TestTypeExternalTest AS TestRequested, dbo_dnasendawaylinked.LaboratoryCode AS [Where sent], dbo_dnasendawaylinked.DateSent AS [Date Sent], dbo_specimenreferrallinked.ReferralOrganisation AS [Referring Hospital], [dbo_SpecimenReferralLinked].[FIRSTNAME] & " " & [ReferralPractitioner] AS Clinician, dbo_dnasendawaylinked.ExpectedCost AS [Estimated Cost], dbo_dnatestrequestlinked.DateRequested, dbo_dnatestrequestlinked.TestDescription
FROM (((((dbo_dnatestrequestlinked INNER JOIN dbo_TestLinked ON dbo_dnatestrequestlinked.TestID = dbo_TestLinked.TestID) LEFT JOIN dbo_dnasendawaylinked ON dbo_TestLinked.TestID = dbo_dnasendawaylinked.TestID) LEFT JOIN dbo_specimenlinked ON dbo_TestLinked.SpecimenID = dbo_specimenlinked.SpecimenID) LEFT JOIN dbo_patientlinked ON dbo_specimenlinked.PatientID = dbo_patientlinked.PatientID) LEFT JOIN dbo_specimenreferrallinked ON dbo_TestLinked.SpecimenID = dbo_specimenreferrallinked.SpecimenID) LEFT JOIN dbo_dnanumberlinked ON dbo_TestLinked.SpecimenID = dbo_dnanumberlinked.SpecimenID
WHERE (((dbo_dnatestrequestlinked.DateRequested) Between [Between period starting? eg 2/10/13] And [And period ending? eg 5/10/13 (set end date as the day after the one you want to include)]) AND ((dbo_dnatestrequestlinked.TestDescription)="Export DNA"))
ORDER BY dbo_dnasendawaylinked.DateSent;
