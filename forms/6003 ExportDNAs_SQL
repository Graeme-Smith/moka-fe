SELECT dbo_dna_PatientLinked.PatientTrustID AS [PRU No], dbo_dna_PatientLinked.LastName AS Surname, dbo_dna_PatientLinked.FirstName AS [First name], dbo_dna_SpecimenLinked.SpecimenTrustID AS [Spec no], dbo_dna_DNANumberLinked.DNANo AS [DNA #], " " AS [Vol sent], dbo_dna_DNASendAwayLinked.TestTypeExternalTest AS TestRequested, dbo_dna_DNASendAwayLinked.LaboratoryCode AS [Where sent], dbo_dna_DNASendAwayLinked.DateSent AS [Date Sent], dbo_dna_SpecimenReferralLinked.ReferralOrganisation AS [Referring Hospital], [dbo_dna_SpecimenReferralLinked].[FIRSTNAME] & " " & [ReferralPractitioner] AS Clinician, dbo_dna_DNASendAwayLinked.ExpectedCost AS [Estimated Cost], dbo_dna_DNATestRequestLinked.DateRequested, dbo_dna_DNATestRequestLinked.TestDescription
FROM ((((dbo_dna_DNATestRequestLinked INNER JOIN dbo_dna_TestLinked ON dbo_dna_DNATestRequestLinked.TestID = dbo_dna_TestLinked.TestID) LEFT JOIN dbo_dna_DNASendAwayLinked ON dbo_dna_TestLinked.TestID = dbo_dna_DNASendAwayLinked.TestID) LEFT JOIN dbo_dna_DNANumberLinked ON dbo_dna_TestLinked.SpecimenID = dbo_dna_DNANumberLinked.SpecimenID) LEFT JOIN (dbo_dna_SpecimenLinked LEFT JOIN dbo_dna_PatientLinked ON dbo_dna_SpecimenLinked.PatientID = dbo_dna_PatientLinked.PatientID) ON dbo_dna_TestLinked.SpecimenID = dbo_dna_SpecimenLinked.SpecimenID) LEFT JOIN dbo_dna_SpecimenReferralLinked ON dbo_dna_TestLinked.SpecimenID = dbo_dna_SpecimenReferralLinked.SpecimenID
WHERE (((dbo_dna_DNATestRequestLinked.DateRequested) Between [Between period starting? eg 2/10/13] And [And period ending? eg 5/10/13 (set end date as the day after the one you want to include)]) AND ((dbo_dna_DNATestRequestLinked.TestDescription)="Export DNA"))
ORDER BY dbo_dna_DNASendAwayLinked.DateSent;
