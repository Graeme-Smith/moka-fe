SELECT s_HybIDs.InternalPatientID, s_HybIDs_1.HybID, Referral.Referral, Patients.PatientID, [BookinLastName] & " " & [BookinFirstName] AS Name, s_HybIDs_1.ArrayRunNumber, s_HybIDs_1.Label, ArrayTest.ArrayTestID, s_HybIDs_1.InternalPatientID AS IPID, dbo_Patient.FamilyNo, Patients.CandidateAbnormality AS FollowupRegion
FROM ((((s_HybIDs INNER JOIN s_HybIDs AS s_HybIDs_1 ON s_HybIDs.DNALabellingID = s_HybIDs_1.DNALabellingID) INNER JOIN ArrayTest ON s_HybIDs_1.DNALabellingID = ArrayTest.DNALabellingID) INNER JOIN Patients ON (ArrayTest.InternalPatientID = Patients.InternalPatientID) AND (s_HybIDs_1.InternalPatientID = Patients.InternalPatientID)) INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID) INNER JOIN dbo_Patient ON Patients.PatientID = dbo_Patient.PatientID
ORDER BY s_HybIDs_1.ArrayRunNumber DESC , s_HybIDs_1.Label;
