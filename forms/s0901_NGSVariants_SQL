SELECT NGSVariant.*, Chromosome.Chr, NGSVariantComment.Comment, NGSVariantACMG.NGSVariantACMGID
FROM ((NGSVariant INNER JOIN Chromosome ON NGSVariant.ChrID = Chromosome.ChrID) LEFT JOIN NGSVariantComment ON NGSVariant.NGSVariantCommentID = NGSVariantComment.NGSVariantCommentID) LEFT JOIN NGSVariantACMG ON NGSVariant.NGSVariantID = NGSVariantACMG.NGSVariantID
ORDER BY Chromosome.Sorting, NGSVariant.Position_hg19;
