SELECT NGSTest.InternalPatientID, "Primary" AS Type, GenesHGNC_150205.ApprovedSymbol, GenesHGNC_150205.ApprovedName
FROM NGSTest INNER JOIN (NGSPanelGenes INNER JOIN GenesHGNC_150205 ON NGSPanelGenes.HGNCID = GenesHGNC_150205.HGNCID) ON NGSTest.NGSPanelID = NGSPanelGenes.NGSPanelID
UNION
SELECT NGSTest.InternalPatientID, "Secondary" AS Type, GenesHGNC_150205.ApprovedSymbol, GenesHGNC_150205.ApprovedName
FROM NGSTest INNER JOIN (NGSPanelGenes INNER JOIN GenesHGNC_150205 ON NGSPanelGenes.HGNCID = GenesHGNC_150205.HGNCID) ON NGSTest.NGSPanelID_B = NGSPanelGenes.NGSPanelID
ORDER BY Type, ApprovedSymbol;
