SELECT s_HybIDs.ArrayRunNumber, s_HybIDs.HybID, FailCount.Fail, Referral.Referral, ArrayTest.Priority AS Rush, [BookinLastname] & " " & [BookinFirstname] AS Name, Checker.Initials AS DataEntry, Checker_1.Initials AS DataCheck, Checker_2.Initials AS Analysis, Referral.RunsheetSorting, ArrayTest.ArrayTestID, ArrayTest.RequestedDate, Checker_3.Initials AS [Check]
FROM ((((((((s_HybIDs INNER JOIN ArrayLabelling ON s_HybIDs.DNALabellingID = ArrayLabelling.DNALabellingID) LEFT JOIN FailCount ON ArrayLabelling.FailCountID = FailCount.FailCountID) INNER JOIN ArrayTest ON (ArrayLabelling.DNALabellingID = ArrayTest.DNALabellingID) AND (s_HybIDs.InternalPatientID = ArrayTest.InternalPatientID)) INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID) INNER JOIN Patients ON s_HybIDs.InternalPatientID = Patients.InternalPatientID) LEFT JOIN Checker ON ArrayTest.Check0ID = Checker.Check1ID) LEFT JOIN Checker AS Checker_1 ON ArrayTest.Check05ID = Checker_1.Check1ID) LEFT JOIN Checker AS Checker_2 ON ArrayTest.Check1ID = Checker_2.Check1ID) LEFT JOIN Checker AS Checker_3 ON ArrayTest.Check2ID = Checker_3.Check1ID
WHERE (((s_HybIDs.ArrayRunNumber)=[Hyb run?]))
ORDER BY ArrayTest.Priority, Referral.RunsheetSorting, ArrayTest.RequestedDate;
