SELECT Patients.PatientID, [dbo_Patient_Table].[LASTNAME] & " " & [dbo_Patient_Table].[FIRSTNAME] AS Name, Referral.Referral, dbo_Specimen_Table.DateReceived, DateDiff("d",[dbo_Specimen_Table].[DateReceived],Now()) AS DaysSinceReceipt
FROM (((dbo_Specimen_Table LEFT JOIN dbo_SpecimenReportLinked ON dbo_Specimen_Table.SpecimenID = dbo_SpecimenReportLinked.SpecimenID) INNER JOIN (Patients INNER JOIN ArrayTest ON Patients.InternalPatientID = ArrayTest.InternalPatientID) ON dbo_Specimen_Table.SpecimenTrustID = ArrayTest.SpecimenNo) INNER JOIN dbo_Patient_Table ON dbo_Specimen_Table.PatientID = dbo_Patient_Table.PatientID) INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID
WHERE (((dbo_Specimen_Table.DateReceived)>#1/1/2015#) AND ((ArrayTest.Priority)=True) AND ((dbo_SpecimenReportLinked.AuthorizedByUserInitials) Is Null) AND ((ArrayTest.ReferralID)<>9 And (ArrayTest.ReferralID)<>1199901190 And (ArrayTest.ReferralID)<>1199901206 And (ArrayTest.ReferralID)<>1199901207 And (ArrayTest.ReferralID)<>1199901203 And (ArrayTest.ReferralID)<>1199901193 And (ArrayTest.ReferralID)<>1199901205 And (ArrayTest.ReferralID)<>1199901176 And (ArrayTest.ReferralID)<>1185356886 And (ArrayTest.ReferralID)<>1199901197) AND ((ArrayTest.StatusID)<>1202218787) AND ((ArrayTest.Service)=True))
ORDER BY dbo_Specimen_Table.DateReceived;
