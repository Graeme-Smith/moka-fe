SELECT Patients.s_StatusOverall, Patients.InternalPatientID, Patients.PatientID, Patients.Sexed, [LastName] & " " & [FirstName] AS Name, dbo_patientlinked.DoB, s_GW_PinkNotesHasLimit1.FamilyID, Left([patients].[patientID],6) AS FamilyNo, Left([dbo_patientlinked].[Gender],1) AS Sex
FROM (Patients LEFT JOIN s_GW_PinkNotesHasLimit1 ON Patients.PatientID = s_GW_PinkNotesHasLimit1.PatientID) INNER JOIN dbo_patientlinked ON Patients.PatientID = dbo_patientlinked.PatientTrustID;
