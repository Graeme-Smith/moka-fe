SELECT Patients.s_StatusOverall, Patients.InternalPatientID, Patients.PatientID, Patients.Sexed, [LastName] & " " & [FirstName] AS Name, dbo_Patient_Table.DoB, s_GW_PinkNotesHasLimit1.FamilyID, Left([patients].[patientID],6) AS FamilyNo, gw_GenderTable.GenderShort AS Sex
FROM ((Patients INNER JOIN dbo_Patient_Table ON Patients.PatientID = dbo_Patient_Table.PatientTrustID) LEFT JOIN gw_GenderTable ON dbo_Patient_Table.GenderID = gw_GenderTable.GenderID) LEFT JOIN s_GW_PinkNotesHasLimit1 ON Patients.PatientID = s_GW_PinkNotesHasLimit1.PatientID;
