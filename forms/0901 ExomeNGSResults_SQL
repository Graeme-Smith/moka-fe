SELECT Left([dbo_Patient_Table].[PatientTrustID],6) AS FamilyNoID, NGSTest.NGSTestID, Patients.InternalPatientID, Patients.OverallResultCodeID, Patients.PatientID, Patients.OverallResultComments, Patients.CandidateAbnormality AS FollowupRegion, [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] AS PatientName, [dbo_Patient_Table].[PatientTrustID] & "          " & [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] & "        Sex: " & [gw_GenderTable].[Gender] & [Sexed] & "       DoB: " & [dbo_Patient_Table].[DoB] AS Patient, dbo_Patient_table.DoB, Status.Status AS [WES Test Status], NGSTest.WESBatch, Patients.OverallResultCodeID, Patients.OverallResultComments, ResultCode.ResultCode, Patients.s_StatusOverall
FROM (((NGSTest INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN (dbo_Patient_table LEFT JOIN gw_GenderTable ON dbo_Patient_table.GenderID = gw_GenderTable.GenderID) ON Patients.PatientID = dbo_Patient_table.PatientTrustID) INNER JOIN Status ON NGSTest.StatusID = Status.StatusID) LEFT JOIN ResultCode ON Patients.OverallResultCodeID = ResultCode.ResultCodeID;
