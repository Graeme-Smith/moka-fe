SELECT DISTINCT dbo_PatientLinked.PatientTrustID AS PRU, [dbo_PatientLinked].[LastName] & " " & [dbo_PatientLinked].[Firstname] AS Patient, dbo_SpecimenReferralLinked.ReferralPractitioner AS Referrer, dbo_DNASendAwayLinked.TestTypeExternalTest AS Test, dbo_DNASendAwayLinked.LaboratoryCode AS Lab, dbo_DNASendAwayInvoiceLinked.DateResultReceived AS [Result Date], dbo_DNASendAwayLinked.Notes FROM (((dbo_DNASendAwayLinked INNER JOIN dbo_SpecimenLinked ON dbo_DNASendAwayLinked.SpecimenID = dbo_SpecimenLinked.SpecimenID) INNER JOIN dbo_PatientLinked ON dbo_SpecimenLinked.PatientID = dbo_PatientLinked.PatientID) INNER JOIN dbo_SpecimenReferralLinked ON dbo_SpecimenLinked.SpecimenID = dbo_SpecimenReferralLinked.SpecimenID) INNER JOIN dbo_DNASendAwayInvoiceLinked ON dbo_DNASendAwayLinked.SendAwayID = dbo_DNASendAwayInvoiceLinked.SendAwayID WHERE (((dbo_DNASendAwayInvoiceLinked.DateResultReceived) Between [Start of date range?] And [End of data range?]) AND ((dbo_DNASendAwayLinked.LaboratoryID)<>397) AND ((dbo_SpecimenReferralLinked.Department)="Clinical Genetics")) ORDER BY dbo_DNASendAwayInvoiceLinked.DateResultReceived;
