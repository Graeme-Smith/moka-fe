TRANSFORM Count(ArrayTest.InternalPatientID) AS CountOfInternalPatientID SELECT Format([ArrayTest].[RequestedDate],"yyyy-mm") AS [Month] FROM ArrayTest INNER JOIN Checker ON ArrayTest.BookedByID = Checker.Check1ID WHERE (((Checker.Initials)="MRC" Or (Checker.Initials)="RB" Or (Checker.Initials)="HT" Or (Checker.Initials)="RP" Or (Checker.Initials)="CR" Or (Checker.Initials)="MJF") AND ((ArrayTest.RequestedDate)>Now()-770)) GROUP BY Format([ArrayTest].[RequestedDate],"yyyy-mm") ORDER BY Format([ArrayTest].[RequestedDate],"yyyy-mm") DESC  PIVOT Checker.Initials;
