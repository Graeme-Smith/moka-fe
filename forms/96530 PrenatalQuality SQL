SELECT DISTINCT ArrayTest.SpecimenNo, dbo_specimenlinked.SpecimenType, dbo_specimenlinked.DateReceived, ResultCode.ResultCode FROM (ArrayTest INNER JOIN ResultCode ON ArrayTest.ArrayResultCodeID = ResultCode.ResultCodeID) INNER JOIN dbo_specimenlinked ON ArrayTest.SpecimenNo = dbo_specimenlinked.SpecimenTrustID WHERE (((dbo_specimenlinked.DateReceived) Between [Start of period] And [End of period]) AND ((ArrayTest.ReferralID)=1199901185 Or (ArrayTest.ReferralID)=1199901188 Or (ArrayTest.ReferralID)=1199901202)) ORDER BY dbo_specimenlinked.DateReceived DESC;
