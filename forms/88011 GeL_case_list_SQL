SELECT dbo_patientlinked.PatientTrustID, NGSTest.GELProbandID, NGSTest.IRID, dbo_patientlinked.LastName, dbo_patientlinked.FirstName, dbo_patientlinked.DoB, Referral.Referral, NGSTest.DateRequested, Status.Status, Checker.Name AS [Referring Clinician], Checker_1.Initials AS [Check 1], NGSTest.Check1Date, Checker_2.Initials AS [Check 2], NGSTest.Check2Date, Checker_3.Initials AS Reported, NGSTest.Check3Date, Checker_4.Initials AS Authorised, NGSTest.Check4Date, NGSTest.Priority, ResultCode.ResultCode, NGSTest.MDTdate, NGSTest.GeL_case_flags, Item.Item AS Block, NGSTest.StatusID, NGSTest.Check1ID, NGSTest.Check2ID, NGSTest.Check3ID, NGSTest.Check4ID, NGSTest.BookBy, NGSTest.NGSTestID
FROM ((((((((((NGSTest INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) INNER JOIN dbo_patientlinked ON Patients.PatientID = dbo_patientlinked.PatientTrustID) INNER JOIN Referral ON NGSTest.ReferralID = Referral.ReferralID) INNER JOIN Status ON NGSTest.StatusID = Status.StatusID) LEFT JOIN Checker ON NGSTest.BookBy = Checker.Check1ID) LEFT JOIN Checker AS Checker_1 ON NGSTest.Check1ID = Checker_1.Check1ID) LEFT JOIN Checker AS Checker_2 ON NGSTest.Check2ID = Checker_2.Check1ID) LEFT JOIN Checker AS Checker_3 ON NGSTest.Check3ID = Checker_3.Check1ID) LEFT JOIN Checker AS Checker_4 ON NGSTest.Check4ID = Checker_4.Check1ID) LEFT JOIN ResultCode ON NGSTest.ResultCode = ResultCode.ResultCodeID) LEFT JOIN Item ON NGSTest.BlockAutomatedReporting = Item.ItemID
WHERE (((NGSTest.ReferralID)=1199901218))
ORDER BY NGSTest.Priority, NGSTest.DateRequested;
