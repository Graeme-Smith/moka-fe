SELECT Patients.InternalPatientID, Patients.PatientID, dbo_dnanumberlinked.DNANo, dbo_dnaspecimenlinked.Concentration, dbo_dnaspecimenlinked.DateReceived, dbo_dnaspecimenlinked.DatePrepared, dbo_dnaspecimenlinked.SpecimenTrustID AS SpecimenNo, dbo_dnaspecimenlinked.SpecimenType
FROM (Patients INNER JOIN dbo_patientlinked ON Patients.PatientID = dbo_patientlinked.PatientTrustID) INNER JOIN (dbo_dnanumberlinked RIGHT JOIN dbo_dnaspecimenlinked ON dbo_dnanumberlinked.SpecimenID = dbo_dnaspecimenlinked.SpecimenID) ON dbo_patientlinked.PatientID = dbo_dnaspecimenlinked.PatientID
ORDER BY dbo_dnaspecimenlinked.DateReceived DESC , dbo_dnaspecimenlinked.DatePrepared DESC;
