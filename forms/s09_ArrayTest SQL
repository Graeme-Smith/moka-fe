SELECT ArrayTest.ArrayTestID, ArrayTest.InternalPatientID, ArrayTest.StatusID, ArrayTest.ArrayResultCodeID, ArrayTest.ArrayResult, ArrayTest.ArrayResultComment, ArrayTest.DNALabellingID, ArrayTest.DyeSwapID, ArrayTest.Check0ID, ArrayTest.Check0Date, ArrayTest.Check05ID, ArrayTest.Check05Date, ArrayTest.Check1ID, ArrayTest.Check1Date, ArrayTest.Check2ID, ArrayTest.ArrayResultDate, ArrayTest.Check3ID, ArrayTest.Check3Date, ArrayTest.RequestedDate, ArrayTest.ArrayResultBuild, ArrayTest.SpecimenNo, ArrayTest.Priority, Referral.ReferralID, Referral.Referral, ArrayTestAnalysisType.ArrayTestAnalysisType, "data not accessible" AS SpecimenType, "data not accessible" AS RequestBy, Patients.PatientID, [Patients].[BookinLastName] & " " & [Patients].[BookinFirstname] AS Name, Left([PatientID],6) AS FamilyTrustID
FROM ((ArrayTest INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID) INNER JOIN ArrayTestAnalysisType ON ArrayTest.ArrayTestAnalysisTypeID = ArrayTestAnalysisType.ArrayTestAnalysisTypeID) INNER JOIN Patients ON ArrayTest.InternalPatientID = Patients.InternalPatientID;
