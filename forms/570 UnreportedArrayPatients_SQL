SELECT Patients.PatientID, Status.Status AS PatientStatus, Status_1.Status AS ArrayStatus, ArrayTest.Priority, ArrayTest.SpecimenNo AS ArraySpecimenNo, [LASTNAME] & " " & [FIRSTNAME] AS Name, Patients.InternalPatientID, DateDiff("d",[dbo_Specimenlinked].[DateReceived],Now()) AS DaysSinceReceipt
FROM ((((Patients INNER JOIN ArrayTest ON Patients.InternalPatientID = ArrayTest.InternalPatientID) INNER JOIN Status ON Patients.s_StatusOverall = Status.StatusID) INNER JOIN Status AS Status_1 ON ArrayTest.StatusID = Status_1.StatusID) INNER JOIN dbo_Patient ON Patients.PatientID = dbo_Patient.PatientID) LEFT JOIN (dbo_specimenlinked LEFT JOIN dbo_specimenreportlinked ON dbo_specimenlinked.SpecimenID = dbo_specimenreportlinked.SpecimenID) ON ArrayTest.SpecimenNo = dbo_specimenlinked.SpecimenTrustID
WHERE (((ArrayTest.RequestedDate)>Now()-365 And (ArrayTest.RequestedDate)<=Now()-35) AND ((ArrayTest.Service)=True) AND ((Patients.s_StatusOverall)<>1202218787) AND ((ArrayTest.StatusID)<>1202218787) AND ((ArrayTest.ReferralID)<>9 And (ArrayTest.ReferralID)<>1199901190 And (ArrayTest.ReferralID)<>1199901206 And (ArrayTest.ReferralID)<>1199901207 And (ArrayTest.ReferralID)<>1199901203 And (ArrayTest.ReferralID)<>1199901193 And (ArrayTest.ReferralID)<>1199901205 And (ArrayTest.ReferralID)<>1199901176 And (ArrayTest.ReferralID)<>1185356886 And (ArrayTest.ReferralID)<>1199901197 And (ArrayTest.ReferralID)<>1199901184 And (ArrayTest.ReferralID)<>1199901148) AND ((dbo_specimenreportlinked.DateAuthorized) Is Null))
ORDER BY ArrayTest.RequestedDate, DateDiff("d",[dbo_Specimenlinked].[DateReceived],Now()) DESC;
