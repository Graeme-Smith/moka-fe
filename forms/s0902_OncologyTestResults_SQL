SELECT Checker.Name AS BookedBy, NGSTest.NGSTestID, Patients.PatientID, Referral.Referral, dbo_Patient_table.LastName, NGSTest.Check1Date, NGSTest.Result, NGSTest.ResultBuild, NGSTest.ResultCode, NGSTest.ResultComment, NGSTest.StatusID, NGSTest.Check2Date, Patients.InternalPatientID, NGSTest.DateRequested, NGSTest.Check1ID, NGSTest.Check2ID, dbo_specimenlinked.SpecimenTrustID, OncologyLab.EK
FROM (((((((NGSTest LEFT JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) LEFT JOIN Referral ON NGSTest.ReferralID = Referral.ReferralID) LEFT JOIN Checker ON NGSTest.BookBy = Checker.Check1ID) LEFT JOIN dbo_Patient_table ON Patients.PatientID = dbo_Patient_table.PatientTrustID) LEFT JOIN Checker AS Checker_1 ON NGSTest.Check1ID = Checker_1.Check1ID) LEFT JOIN Checker AS Checker_2 ON NGSTest.Check2ID = Checker_2.Check1ID) INNER JOIN OncologyLab ON NGSTest.OncologyLabID = OncologyLab.OncologyLabID) INNER JOIN dbo_specimenlinked ON NGSTest.GWSpecID = dbo_specimenlinked.SpecimenID;
