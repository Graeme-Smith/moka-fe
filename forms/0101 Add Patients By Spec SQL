SELECT dbo_Specimen_Table.SpecimenTrustID, dbo_Specimen_Table.DateReceived, gw_SpecimenType.SpecimenTypeCode, dbo_Patient_Table.PatientTrustID AS PatientID, dbo_Patient_Table.LastName, dbo_Patient_Table.FirstName, dbo_Patient_Table.DoB, gw_GenderTable.GenderShort AS Sex, "unknown" AS CreatedBy, #10/1/1901# AS CreatedDate, Patients.s_StatusOverall, Patients.InternalPatientID, Patients.ePGDCycleID, Patients.MokaCreated, Patients.MokaCreatedBy, Patients.BookinLastName, Patients.BookinFirstName, Patients.BookinSex, Patients.BookinDOB, Patients.BookinCreatedBy, Patients.BookinCreatedDate
FROM (((dbo_Patient_Table LEFT JOIN gw_GenderTable ON dbo_Patient_Table.GenderID = gw_GenderTable.GenderID) LEFT JOIN Patients ON dbo_Patient_Table.PatientTrustID = Patients.PatientID) INNER JOIN dbo_Specimen_Table ON dbo_Patient_Table.PatientID = dbo_Specimen_Table.PatientID) INNER JOIN gw_SpecimenType ON dbo_Specimen_Table.SpecimenTypeID = gw_SpecimenType.SpecimenTypeID
WHERE (((dbo_Specimen_Table.SpecimenTrustID)=[Specimen number?]));
