Option Compare Database

Private Sub BookingAuthorisedByID_Click()
    Dim dt As String
    dt = Format(Now, "dd/mmm/yyyy Hh:Nn:ss")
    Me![BookingAuthorisedDate].Value = dt
    ' Debug.Print Me![BookingAuthorisedDate].Value
    ' Check is approver ID is the same as the requester ID. Prompt user to acknowledge.

    If Me![BookingAuthorisedByID].Value = Me![Check1ID].Value Then
    msga = MsgBox("The referral clinician ID is the same as the approver ID." & vbNewLine & "Do you wish to proceed?", vbYesNo Or vbInformation, "INFORMATION")
        If msga = vbNo Then
            Me![BookingAuthorisedByID].Value = Null
            Me![BookingAuthorisedDate].Value = Null
        End If
    End If
End Sub
