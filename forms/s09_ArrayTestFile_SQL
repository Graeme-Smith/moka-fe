SELECT IIf([ArrayTestFile].[ArrayTestFile] Is Not Null,"OLE object",[ArrayTestFile].[ArrayTestFilepath]) AS linkDescription, ArrayTestFile.ArrayTestID, ArrayTestFile.ArrayTestFile, ArrayTestFile.Description, ArrayTestFile.Date, ArrayTestFile.ArrayTestFilepath, ArrayTestFile.ArrayTestFileID FROM ArrayTestFile WHERE (((ArrayTestFile.ArrayTestID)=[Forms]![09 ArrayResultsByPostition]![ArrayTestID])) ORDER BY ArrayTestFile.Date;