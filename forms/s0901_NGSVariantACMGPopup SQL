SELECT NGSVariantACMG.*, [dbo_Patient_Table].[PatientTrustID] & "          " & [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] & "       Sex: " & [gw_GenderTable].[Gender] & "       DoB: " & [dbo_Patient_Table].[DoB] AS Patient, dbo_Patient_Table.DoB, dbo_Patient_Table.PatientTrustID, gw_GenderTable.Gender, Chromosome.Chromosome, NGSVariant.Position_hg19, NGSVariant.ref, NGSVariant.alt, NGSVariantComment.Comment AS NGSVariant_comment, NGSVariantComment.NGSVariantCommentID, NGSTest.InternalPatientID, "chr" & [Chromosome].[Chromosome] & ":" & [NGSVariant].[Position_hg19] & "          Ref: " & [NGSVariant].[ref] & "          Alt: " & [NGSVariant].[alt] AS Variant, NGSVariant.Classification
FROM (((dbo_Patient_Table INNER JOIN (((NGSTest INNER JOIN NGSVariant ON NGSTest.NGSTestID = NGSVariant.NGSTestID) INNER JOIN NGSVariantACMG ON NGSVariant.NGSVariantID = NGSVariantACMG.NGSVariantID) INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) ON dbo_Patient_Table.PatientTrustID = Patients.PatientID) INNER JOIN gw_GenderTable ON dbo_Patient_Table.GenderID = gw_GenderTable.GenderID) INNER JOIN Chromosome ON NGSVariant.ChrID = Chromosome.ChrID) INNER JOIN NGSVariantComment ON NGSVariant.NGSVariantCommentID = NGSVariantComment.NGSVariantCommentID;
