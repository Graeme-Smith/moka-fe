SELECT NGSVariantACMG.*, [dbo_Patient_Table].[PatientTrustID] & "          " & [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] & "       Sex: " & [gw_GenderTable].[Gender] & "       DoB: " & [dbo_Patient_Table].[DoB] AS Patient, dbo_Patient_Table.DoB, dbo_Patient_Table.PatientTrustID, gw_GenderTable.Gender, Chromosome.Chr, NGSVariant.Position_hg19, NGSVariant.ref, NGSVariant.alt, NGSVariantComment.Comment AS NGSVariant_comment, NGSVariantComment.NGSVariantCommentID, NGSTest.InternalPatientID, "b37          chr" & [Chromosome].[Chr] & ":" & [NGSVariant].[Position_hg19] & "          Ref: " & [NGSVariant].[ref] & "          Alt: " & [NGSVariant].[alt] AS Variant_b37, NGSVariant.Classification, "b38          chr" & [Chromosome_1].[Chr] & ":" & [NGSVariant].[position_38] & "          Ref: " & [NGSVariant].[ref_38] & "          Alt: " & [NGSVariant].[alt_38] AS Variant_b38, NGSVariant.Gene
FROM ((((dbo_Patient_Table INNER JOIN (((NGSTest INNER JOIN NGSVariant ON NGSTest.NGSTestID = NGSVariant.NGSTestID) INNER JOIN NGSVariantACMG ON NGSVariant.NGSVariantID = NGSVariantACMG.NGSVariantID) INNER JOIN Patients ON NGSTest.InternalPatientID = Patients.InternalPatientID) ON dbo_Patient_Table.PatientTrustID = Patients.PatientID) LEFT JOIN gw_GenderTable ON dbo_Patient_Table.GenderID = gw_GenderTable.GenderID) LEFT JOIN Chromosome ON NGSVariant.ChrID = Chromosome.ChrID) INNER JOIN NGSVariantComment ON NGSVariant.NGSVariantCommentID = NGSVariantComment.NGSVariantCommentID) LEFT JOIN Chromosome AS Chromosome_1 ON NGSVariant.chr_id_38 = Chromosome_1.ChrID;
