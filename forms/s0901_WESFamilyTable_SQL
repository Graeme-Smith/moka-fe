SELECT Left([dbo_Patient_Table].[PatientTrustID],6) AS FamilyNoID, dbo_Patient_table.PatientTrustID AS PatientPRU, [dbo_Patient_Table].[LastName] & ", " & [dbo_Patient_Table].[FirstName] AS Name, dbo_Patient_table.DoB, gw_GenderTable.GenderShort AS Sex, dbo_DNANumber2016.DNANo AS DNA, Status.Status AS PatientStatusIfInMoka, [dbo_Specimen_Table].[SpecimenTrustID] & " " & [gw_SpecimenType].[SpecimenType] AS Specimen, Status_1.Status AS NGSTestStatus
FROM (((((((dbo_Patient_table LEFT JOIN dbo_Specimen_table ON dbo_Patient_table.PatientID = dbo_Specimen_table.PatientID) LEFT JOIN gw_GenderTable ON dbo_Patient_table.GenderID = gw_GenderTable.GenderID) LEFT JOIN gw_SpecimenType ON dbo_Specimen_table.SpecimenTypeID = gw_SpecimenType.SpecimenTypeID) LEFT JOIN Patients ON dbo_Patient_table.PatientTrustID = Patients.PatientID) LEFT JOIN Status ON Patients.s_StatusOverall = Status.StatusID) LEFT JOIN dbo_DNANumber2016 ON dbo_Specimen_table.SpecimenID = dbo_DNANumber2016.SpecimenID) LEFT JOIN NGSTest ON Patients.InternalPatientID = NGSTest.InternalPatientID) LEFT JOIN Status AS Status_1 ON NGSTest.StatusID = Status_1.StatusID
WHERE (((Left([dbo_Patient_Table].[PatientTrustID],6))=[forms]![s0901_WESFamily]![txt_FamID]));
