SELECT s_OPMS_97030.Month, s_OPMS_97030.Received, s_OPMS_97031.Completed, s_OPMS_97048.Activity, s_OPMS_97033.[42days], s_OPMS_97033.[25days], s_OPMS_97034.Errors, s_OPMS_97035.Fails, s_OPMS_97033.[42daysN], s_OPMS_97033.[25daysN], Left([s_OPMS_97030].[month],4) AS Y, Mid([s_OPMS_97030].[month],6,2) AS M, s_OPMS_97045.Followups, s_OPMS_97044.Inheritance, s_OPMS_97047.PatMat, s_OPMS_97046.Sexing
FROM ((((((s_OPMS_97048 INNER JOIN ((s_OPMS_97033 LEFT JOIN s_OPMS_97034 ON s_OPMS_97033.Month = s_OPMS_97034.Month) LEFT JOIN s_OPMS_97035 ON s_OPMS_97033.Month = s_OPMS_97035.Month) ON s_OPMS_97048.Month = s_OPMS_97033.Month) INNER JOIN s_OPMS_97031 ON s_OPMS_97033.Month = s_OPMS_97031.Month) INNER JOIN s_OPMS_97030 ON s_OPMS_97033.Month = s_OPMS_97030.Month) INNER JOIN s_OPMS_97047 ON s_OPMS_97033.Month = s_OPMS_97047.Month) INNER JOIN s_OPMS_97046 ON s_OPMS_97033.Month = s_OPMS_97046.Month) LEFT JOIN s_OPMS_97045 ON s_OPMS_97033.Month = s_OPMS_97045.Month) INNER JOIN s_OPMS_97044 ON s_OPMS_97033.Month = s_OPMS_97044.Month
ORDER BY Left([s_OPMS_97030].[month],4) DESC , Mid([s_OPMS_97030].[month],6,2) DESC;
