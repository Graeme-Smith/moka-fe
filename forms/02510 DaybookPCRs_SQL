SELECT QFPCRRequest.QFPCRRequestID, QFPCRRequest.InternalPatientID, QFPCRRequest.SpecimenNo, Referral.Referral, Checker.Initials AS BookedBy, QFPCRRequest.StatusID, QFPCRRequest.Result, QFPCRRequest.Comment, QFPCRRequest.AnalysedByID, QFPCRRequest.AnalysedByDate, QFPCRRequest.CheckedByID, QFPCRRequest.ResultDate, QFPCRRequest.RequestedDate, QFPCRRequest.ReferralID, [dbo_SpecimenDayBook_Table].[LastName] & " " & [dbo_SpecimenDayBook_Table].[FirstName] AS Name, QFPCRRequest.RequestedDate, QFPCRRequest.Result AS RID FROM (((QFPCRRequest INNER JOIN Referral ON QFPCRRequest.ReferralID = Referral.ReferralID) INNER JOIN Checker ON QFPCRRequest.BookedByID = Checker.Check1ID) INNER JOIN dbo_SpecimenDayBook_Table ON QFPCRRequest.GWSpecID = dbo_SpecimenDayBook_Table.SpecimenID) INNER JOIN dbo_Specimen_Table ON dbo_SpecimenDayBook_Table.SpecimenID = dbo_Specimen_Table.SpecimenID WHERE (((QFPCRRequest.InternalPatientID) Is Null) AND ((QFPCRRequest.AnalysedByID) Is Null)) OR (((QFPCRRequest.InternalPatientID) Is Null) AND ((QFPCRRequest.CheckedByID) Is Null)) ORDER BY QFPCRRequest.RequestedDate;
