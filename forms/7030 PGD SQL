SELECT Referral.Referral, ArrayTest.ReferralID, Patients.PatientID, Patients.BookinLastName, Patients.BookinFirstName, s_HybIDs.HybID, ArrayTest.ArrayResult, ArrayTest.ArrayResultComment
FROM ((ArrayTest INNER JOIN Referral ON ArrayTest.ReferralID = Referral.ReferralID) INNER JOIN s_HybIDs ON ArrayTest.DNALabellingID = s_HybIDs.DNALabellingID) INNER JOIN Patients ON ArrayTest.InternalPatientID = Patients.InternalPatientID
WHERE (((ArrayTest.ReferralID)=1199901190 Or (ArrayTest.ReferralID)=1199901206 Or (ArrayTest.ReferralID)=1199901207 Or (ArrayTest.ReferralID)=1199901203 Or (ArrayTest.ReferralID)=1199901193 Or (ArrayTest.ReferralID)=1199901205) AND ((Patients.BookinLastName)<>"PROMEGA" And (Patients.BookinLastName)<>"SUREPLEX") AND ((ArrayTest.StatusID)<>1202218787) AND ((s_HybIDs.Label)="5-Cy-5"))
ORDER BY Patients.PatientID, s_HybIDs.ArrayRunNumber, s_HybIDs.ArrayBarCode, s_HybIDs.Subarray;
