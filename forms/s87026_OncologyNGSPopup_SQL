SELECT dbo_specimenlinked.SpecimenID, Patients.InternalPatientID, OncologyLab.OncologyLabID, OncologyLab.EK, dbo_patientlinked.PatientTrustID, [OncologyLab].[EK] & "        " & [dbo_specimenlinked].[SpecimenTrustID] & "        " & [dbo_patientlinked].[LastName] & " " & [FirstName] AS Name, dbo_specimenlinked.SpecimenTrustID, "Specimen Number: " & [dbo_specimenlinked].[SpecimenTrustID] & "        EK Number: " & [OncologyLab].[EK] AS Sample
FROM OncologyLab INNER JOIN ((dbo_specimenlinked INNER JOIN dbo_patientlinked ON dbo_specimenlinked.PatientID = dbo_patientlinked.PatientID) INNER JOIN Patients ON dbo_patientlinked.PatientTrustID = Patients.PatientID) ON OncologyLab.GWSpecID = dbo_specimenlinked.SpecimenID;
