SELECT dbo_specimenlinked.SpecimenID, dbo_specimenlinked.SpecimenTrustID AS SpecimenNumber, dbo_specimenlinked.SpecimenType AS Type, Patients.InternalPatientID, dbo_patientlinked.PatientTrustID AS PatientID, dbo_patientlinked.DoB, dbo_patientlinked.NHSNo, dbo_patientlinked.LastName, dbo_patientlinked.FirstName, dbo_patientlinked.CreatedDate, dbo_patientlinked.CreatedByID, gw_GenderTable.GenderShort, gw_GenderTable.Gender, [dbo_patientlinked].[PatientTrustID] & "          " & [dbo_patientlinked].[LastName] & " " & [dbo_patientlinked].[FirstName] & "       Sex: " & [gw_GenderTable].[Gender] & "       DoB: " & [dbo_patientlinked].[DoB] AS Patient
FROM ((dbo_specimenlinked INNER JOIN dbo_patientlinked ON dbo_specimenlinked.PatientID = dbo_patientlinked.PatientID) LEFT JOIN (Patients LEFT JOIN Status ON Patients.s_StatusOverall = Status.StatusID) ON dbo_patientlinked.PatientTrustID = Patients.PatientID) LEFT JOIN gw_GenderTable ON dbo_patientlinked.GenderID = gw_GenderTable.GenderID
WHERE (((dbo_specimenlinked.SpecimenTrustID)=[Specimen Number?]));
