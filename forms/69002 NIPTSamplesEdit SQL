SELECT NIPTSamples.NIPTSamplesID, NIPTSamples.SpecimenNo, NIPTSamples.GWSpecID, NIPTSamples.Referral, NIPTSamples.Drawn, NIPTSamples.Received, NIPTSamples.SentToVerinata, NIPTSamples.RecivedByVerinata, NIPTSamples.ResultAvailable, NIPTSamples.Reported, DateDiff("d",[Drawn],[Reported]) AS TAT, NIPTSamples.Gestation, NIPTSamples.Comment, dbo_Specimen_Table.DateReceived, dbo_Specimen_Table.SpecimenTrustID, dbo_Patient_Table.PatientTrustID, [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[FirstName] AS Name, dbo_Patient_Table.DoB, dbo_ReferringOrganisation_Table.Organisation, dbo_SpecimenReferral_Table.ReferralOrganisationID
FROM (((NIPTSamples INNER JOIN dbo_Specimen_Table ON NIPTSamples.GWSpecID = dbo_Specimen_Table.SpecimenID) INNER JOIN dbo_Patient_Table ON dbo_Specimen_Table.PatientID = dbo_Patient_Table.PatientID) INNER JOIN dbo_SpecimenDayBook_Table ON NIPTSamples.GWSpecID = dbo_SpecimenDayBook_Table.SpecimenID) INNER JOIN (dbo_SpecimenReferral_Table INNER JOIN dbo_ReferringOrganisation_Table ON dbo_SpecimenReferral_Table.OrganisationID = dbo_ReferringOrganisation_Table.OrganisationID) ON NIPTSamples.GWSpecID = dbo_SpecimenReferral_Table.SpecimenID
ORDER BY NIPTSamples.Drawn DESC;
