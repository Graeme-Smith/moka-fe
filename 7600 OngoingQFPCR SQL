SELECT QFPCRRequest.RequestedDate, QFPCRRequest.ReferralID, QFPCRRequest.InternalPatientID, QFPCRRequest.RequestedByID, QFPCRRequest.BookedByID, QFPCRRequest.BookingCheckedByID, QFPCRRequest.StatusID, QFPCRRequest.Result, QFPCRRequest.ResultDate, QFPCRRequest.Comment, QFPCRRequest.AnalysedByID, QFPCRRequest.CheckedByID, QFPCRRequest.QFPCRRequestID, Patients.PatientID, [dbo_Patient_Table].[LastName] & " " & [dbo_Patient_Table].[Firstname] AS Name, Status_1.Status AS Rush, QFPCRRequest.AnalysedByDate, Patients.s_StatusOverall AS PatientStatus, Patients.Sexed, gw_SpecimenType.SpecimenTypeCode
FROM (((QFPCRRequest INNER JOIN Patients ON QFPCRRequest.InternalPatientID = Patients.InternalPatientID) INNER JOIN Status AS Status_1 ON QFPCRRequest.Rush = Status_1.StatusID) LEFT JOIN (dbo_Specimen_Table LEFT JOIN gw_SpecimenType ON dbo_Specimen_Table.SpecimenTypeID = gw_SpecimenType.SpecimenTypeID) ON QFPCRRequest.SpecimenNo = dbo_Specimen_Table.SpecimenTrustID) INNER JOIN dbo_Patient_Table ON Patients.PatientID = dbo_Patient_Table.PatientTrustID
WHERE (((QFPCRRequest.StatusID)<>4 And (QFPCRRequest.StatusID)<>1202218787))
ORDER BY QFPCRRequest.RequestedDate DESC;
