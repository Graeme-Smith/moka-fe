      ,[DateReceived]
      ,[UrgencyID]
      ,[Urgency]
      ,[Stage]
      ,[SpecimenTypeID]
      ,[SpecimenTypeCode]
      ,[SpecimenType]
      ,[Prenatal]
      ,[SpecimenSubTypeID]
      ,[SpecimenSubType]
      ,[ContainerTypeID]
      ,[ContainerTypeCode]
      ,[ContainerType]
      ,[ContainerCount]
      ,[ReferralCategoryID]
      ,[ReferralCategory]
      ,[CreatedByID]
      ,[InitialsCreatedBy]
      ,[CreatedDate]
      ,[RowVersion]
      ,[DNASpecimenTrustID]
      ,[Active]
      ,[ReferralReason]
      ,[Concentration]
      ,[NewReferral]
      ,[ClinicSpecimen]
      ,[PreparedByID]
      ,[InitialsPreparedBy]
      ,[DatePrepared]
      ,[DateActivated]
      ,[DateChecked]
      ,[DNASpecimenRowVersion]
      ,[SampleCondition]
  FROM [SV-PR-GENWORK].[GeneWorks].[dbo].[DNASpecimenLinked]

